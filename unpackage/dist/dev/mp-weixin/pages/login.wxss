.changeCompy {
  padding-bottom: 150rpx;
}
.changeCompy .titleBox {
    width: 100%;
    height: 80rpx;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    font-size: 28rpx;
    color: #888888;
    padding: 30rpx 40rpx;
    line-height: 40rpx;
}
.changeCompy .titleBox .inline {
      display: inline-block;
      color: #000000;
}
.changeCompy .eachCompyBox {
    width: 100%;
    height: 87rpx;
    padding-left: 40rpx;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    background: white;
}
.changeCompy .eachCompyBox .eachCompy {
      width: 100%;
      height: 98%;
      border-bottom: 1px solid #E5E5E5;
      font-size: 34rpx;
      line-height: 80rpx;
}
.changeCompy .eachCompyBox .eachCompy .radios {
        float: left;
        margin-right: 10rpx;
}
.changeCompy .bottomBox {
    width: 100%;
    height: 140rpx;
    background: #FBFCFB;
    position: fixed;
    left: 0;
    bottom: 0;
}
.changeCompy .bottomBox .loginBtn {
      -webkit-border-radius: 10rpx;
              border-radius: 10rpx;
      color: white;
      width: -webkit-calc(100% - 80rpx);
      width: calc(100% - 80rpx);
      height: 100rpx;
      line-height: 94rpx;
      text-algn: center;
      margin-top: 20rpx;
      background: -webkit-gradient(linear, right top, left top, from(#431aff), to(#203ad2));
      background: -webkit-linear-gradient(right, #431aff 0%, #203ad2 100%);
      background: -o-linear-gradient(right, #431aff 0%, #203ad2 100%);
      background: linear-gradient(270deg, #431aff 0%, #203ad2 100%);
}
.changeCompy .bottomBox .disableds {
      background: #999999;
}
.eachCompy:last-child {
  border-bottom: none;
}

