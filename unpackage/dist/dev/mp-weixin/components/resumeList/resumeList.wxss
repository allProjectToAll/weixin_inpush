.fast-search-box-relative {
  position: relative;
}
.resumeListBody {
  width: 100%;
  overflow: auto;
}
.resumeListBody .eachReceiveResume {
    width: 100%;
    height: 253rpx;
    background: white;
    padding: 30rpx 0 0 0;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    margin-top: 20rpx;
}
.resumeListBody .eachReceiveResume .eachReceiveResumeTop {
      width: -webkit-calc(100% - 40rpx);
      width: calc(100% - 40rpx);
      border-bottom: 1px solid #E5E5E5;
      height: 142rpx;
      margin-left: 40rpx;
}
.resumeListBody .eachReceiveResume .eachReceiveResumeTop .eachReceiveResumeTopName {
        float: left;
        font-size: 34rpx;
        font-weight: bolder;
        color: #222222;
}
.resumeListBody .eachReceiveResume .eachReceiveResumeTop .eachReceiveResumeTopTime {
        float: right;
        color: rgba(0, 0, 0, 0.65);
        font-size: 26rpx;
        margin-right: 40rpx;
}
.resumeListBody .eachReceiveResume .eachReceiveResumeTop .eachReceiveResumeTitle {
        float: left;
        font-size: 28rpx;
        color: #999999;
        margin-right: 40rpx;
}
.resumeListBody .eachReceiveResume .eachReceiveResumeTop .eachReceiveResumeValue {
        float: left;
        font-size: 28rpx;
        color: #595959;
        float: left;
}
.resumeListBody .eachReceiveResume .eachReceiveResumeTop .status {
        float: right;
        width: 104rpx;
        height: 44rpx;
        line-height: 44rpx;
        text-align: center;
        -webkit-border-radius: 22rpx;
                border-radius: 22rpx;
        font-size: 24rpx;
        margin: -15rpx 40rpx 0 0;
}
.resumeListBody .eachReceiveResume .eachReceiveResumeTop .statusSucc {
        color: #0CBBA3;
        background: rgba(12, 187, 163, 0.1);
}
.resumeListBody .eachReceiveResume .eachReceiveResumeTop .statusLoad {
        background: rgba(255, 167, 33, 0.1);
        color: #FFA721;
}
.resumeListBody .eachReceiveResume .eachReceiveResumeTop .statusError {
        background: rgba(248, 0, 128, 0.1);
        color: #F80080;
}
.resumeListBody .eachReceiveResume .toCheckResume {
      width: 100%;
      text-align: center;
      line-height: 80rpx;
      font-size: 28rpx;
      color: #222222;
}

